 * @版本：V1.0
 */
@MapperScan("cn.com.dao")
@SpringBootApplication
@EnableScheduling
@ComponentScan("cn.com")
public class App {
	  public static void main( String[] args )
	    {
	        SpringApplication.run(App.class, args);
	    }
}
