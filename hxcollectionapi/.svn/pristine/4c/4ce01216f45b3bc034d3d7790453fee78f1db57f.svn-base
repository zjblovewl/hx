package cn.com.base.vo.wallet;

/**
 * @类功能说明：绑定银行卡请求参数
 * @公司名称：南京华讯方舟通讯设备有限公司
 * @作者：zhoujinbing
 * @创建时间：2018/4/10 上午9:40
 * @版本：V1.0
 */
public class BankRequestParam {

    /**
     * 银行编码 字典表(sys_dictionary  bank_type)
     */
    private String bank_code;

    /**
     * 城市Code
     */
    private String bank_city;

    /**
     * 开户人姓名
     */
    private String name;

    /**
     * 银行卡号
     */
    private String card_no;

    /**
     * 确认卡号
     */
    private String confirm_card_no;

    public String getBank_code() {
        return bank_code;
    }

    public void setBank_code(String bank_code) {
        this.bank_code = bank_code;
    }

    public String getBank_city() {
        return bank_city;
    }

    public void setBank_city(String bank_city) {
        this.bank_city = bank_city;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCard_no() {
        return card_no;
    }

    public void setCard_no(String card_no) {
        this.card_no = card_no;
    }

    public String getConfirm_card_no() {
        return confirm_card_no;
    }

    public void setConfirm_card_no(String confirm_card_no) {
        this.confirm_card_no = confirm_card_no;
    }
}
